        {
          "key": "current",
          "label": "Current Position",
          "type": "text",
          "required": false
        },
        {
          "key": "description",
          "label": "Job Description",
          "type": "list",
          "required": true,
          "placeholder": "â€¢ Developed and maintained web applications..."
        }
      ]
