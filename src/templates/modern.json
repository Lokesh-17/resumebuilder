          "required": true,
          "placeholder": "john@example.com"
        },
        {
          "key": "phone",
          "label": "Phone",
          "type": "phone",
          "required": true,
          "placeholder": "+1 (555) 123-4567"
        },
        {
          "key": "city",
          "label": "City",
          "type": "text",
          "required": false,
          "placeholder": "New York, NY"
        },
        {
          "key": "website",
          "label": "Website",
          "type": "url",
          "required": false,
          "placeholder": "https://johndoe.com"
        },
        {
          "key": "linkedin",
          "label": "LinkedIn",
          "type": "url",
          "required": false,
