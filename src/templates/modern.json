    {
      "key": "projects",
      "label": "Projects",
      "required": false,
      "repeatable": true,
      "fields": [
        {
          "key": "name",
          "label": "Project Name",
          "type": "text",
          "required": true,
          "placeholder": "E-commerce Platform"
