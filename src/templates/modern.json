    {
      "key": "skills",
      "label": "Skills",
      "required": true,
      "repeatable": true,
      "fields": [
        {
          "key": "name",
          "label": "Skill",
          "type": "text",
          "required": true,
          "placeholder": "JavaScript"
        },
        {
          "key": "proficiency",
          "label": "Proficiency",
          "type": "text",
          "required": false,
          "placeholder": "Advanced"
        }
      ]
    },
    {
      "key": "education",
      "label": "Education",
      "required": false,
      "repeatable": true,
      "fields": [
        {
          "key": "degree",
          "label": "Degree",
          "type": "text",
          "required": true,
          "placeholder": "Bachelor of Science in Computer Science"
