        {
          "key": "description",
          "label": "Description",
          "type": "textarea",
          "required": true,
          "placeholder": "Built a full-stack e-commerce platform using React and Node.js..."
        },
        {
          "key": "stack",
          "label": "Tech Stack",
          "type": "list",
          "required": false,
          "placeholder": "React, Node.js, MongoDB"
        }
      ]
    }
