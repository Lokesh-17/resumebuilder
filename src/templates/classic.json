        {
          "key": "institution",
          "label": "Institution",
          "type": "text",
          "required": true,
          "placeholder": "University of Technology"
        },
        {
          "key": "year",
          "label": "Graduation Year",
          "type": "text",
          "required": true,
          "placeholder": "2020"
