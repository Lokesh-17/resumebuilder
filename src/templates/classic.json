  "sections": [
    {
      "key": "contact",
      "label": "Contact Information",
      "required": true,
      "repeatable": false,
      "fields": [
        {
          "key": "fullName",
          "label": "Full Name",
          "type": "text",
          "required": true,
          "placeholder": "John Doe"
        },
        {
          "key": "title",
          "label": "Professional Title",
          "type": "text",
          "required": true,
          "placeholder": "Software Engineer"
        },
        {
          "key": "email",
          "label": "Email",
          "type": "email",
          "required": true,
          "placeholder": "john@example.com"
        },
        {
          "key": "phone",
          "label": "Phone",
          "type": "phone",
          "required": true,
          "placeholder": "+1 (555) 123-4567"
        },
        {
          "key": "city",
          "label": "City",
          "type": "text",
          "required": true,
          "placeholder": "New York, NY"
        },
        {
          "key": "website",
          "label": "Website",
          "type": "url",
          "required": false,
          "placeholder": "https://johndoe.com"
        }
      ]
    },
    {
      "key": "summary",
      "label": "Objective",
