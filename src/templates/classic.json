        {
          "key": "current",
          "label": "Current Position",
          "type": "text",
          "required": false
        },
        {
          "key": "description",
          "label": "Responsibilities",
          "type": "list",
          "required": true,
          "placeholder": "â€¢ Managed a team of 5 developers..."
